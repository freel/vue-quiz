{
  "name": "vue-quiz",
  "version": "0.1.1",
  "description": "Quiz component based on Vue.js",
  "main": "dist/vue-quiz.common.js",
  "module": "dist/vue-quiz.esm.js",
  "unpkg": "dist/vue-quiz.js",
  "repository": "freel/vue-quiz",
  "author": {
    "name": "Kurliuk Pavel",
    "url": "https://freel.me"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "quiz",
    "component",
    "vue-component",
    "html",
    "css",
    "javascript",
    "front-end",
    "web"
  ],
  "scripts": {
